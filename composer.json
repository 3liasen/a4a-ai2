{
    "name": "sevenyellowmonkeys/axs4all-ai",
    "description": "AI-driven accessibility data classification plugin for WordPress.",
    "type": "wordpress-plugin",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/dom-crawler": "^7.0",
        "symfony/css-selector": "^7.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "squizlabs/php_codesniffer": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "Axs4allAi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Axs4allAi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs"
    }
}
